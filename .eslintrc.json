{
    "rules": {
        // "indent": [2, "tab"],
        "quotes": [2,"single"],
        "linebreak-style": [2,"unix"],
        "semi": [2,"always"],
        "no-console": 0,
        "no-debugger": 0,
        "comma-dangle": 0
    },
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "plugins": [
        "react"
    ],
    "globals": {
        "angular": false,
        "buildings": true,
        "events": true,
        "testLog": true,
        "b3": true,
        "Phaser": true,
        "PIXI": true,
        "gameManager": true,
        "uniqueId": true,
        "Opentip": true,
        "$": true,
        "$rootScope": true,
        "performance": true
    }
}